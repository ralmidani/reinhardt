{
  "name": "reinhardt",
  "license": "Apache-2.0",
  "version": "0.1.1",
  "description": "A small, declarative data modeling and validation library for (potentially) any environment, written in TypeScript",
  "main": "index.js",
  "scripts": {
    "prettify": "prettier --write \"**/*.{js,ts}\"",
    "start": "node index.js",
    "test": "jest",
    "test-watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ralmidani/reinhardt.git"
  },
  "keywords": [
    "ORM",
    "MVC",
    "Model",
    "Declarative",
    "Data",
    "Validation"
  ],
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.2",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "jest": "^22.4.3",
    "prettier": "^1.12.1",
    "ts-jest": "^22.4.4",
    "typescript": "^2.8.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
